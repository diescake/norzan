{
  "name": "Norzan",
  "version": "0.1.0",
  "description": "Norzan displays the rest of workable time into a new tab.",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "page_action": {
    "default_icon": {
      "16": "get_started16.png",
      "32": "get_started32.png",
      "48": "get_started48.png",
      "128": "get_started128.png"
    }
  },
  "icons": {
    "16": "get_started16.png",
    "32": "get_started32.png",
    "48": "get_started48.png",
    "128": "get_started128.png"
  },
  "manifest_version": 2
}
